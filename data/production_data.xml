<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
 
        <record id="sequence_ritase_order" model="ir.sequence">
            <field name="name">Ritase</field>
            <field name="code">ritase</field>
            <field name="prefix">RIT/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_hourmeter_order" model="ir.sequence">
            <field name="name">Hourmeter</field>
            <field name="code">hourmeter</field>
            <field name="prefix">HM/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_production_order" model="ir.sequence">
            <field name="name">Production Order</field>
            <field name="code">production_order</field>
            <field name="prefix">PROD/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_production_cop_adjust" model="ir.sequence">
            <field name="name">Cop Adjust</field>
            <field name="code">cop_adjust</field>
            <field name="prefix">COP/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="dp_production" model="decimal.precision">
            <field name="name">Production</field>
            <field name="digits">3</field>
        </record>

        <!-- Procurement rules and routes -->
        <record id="route_warehouse0_mining_production" model='stock.location.route'>
            <field name="name">Production</field>
            <field name="sequence">5</field>
        </record>

        <!-- <record id="uom_ritase" model="product.uom">
            <field name="name">Rit</field>
            <field name="category_id">Unit</field>
            <field name="uom_type">reference/</field>
            <field name="rounding">0.01</field>
        </record> -->

    </data>
</odoo>